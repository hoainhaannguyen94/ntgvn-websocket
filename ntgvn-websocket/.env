# APP
VERSION = 1.0.0

# LISTENING
HOSTNAME = localhost
HTTP_PORT = 8081

# REQUEST BODY SIZE
MAX_REQUEST_BODY_SIZE = 50mb

# DOMAIN
DOMAIN = ntgvn

# NODE ENV
NODE_ENV = dev # prod

# API
API_VERSION = v1

# LOGGER
ENABLE_LOGGER_CONSOLE = true
#ENABLE_LOGGER_FILE   = true

# MONGO
ENABLE_MONGO = true
MONGO_URI = mongodb://0.0.0.0:27017/ # Docker: mongodb://local-mongo:27017/

# JWT
JWT_SECRET = 
JWT_EXPIRATION_TIME = 365days
JWT_ALGORITHM = HS256
